{
  "name": "@coko/client",
  "version": "0.0.14",
  "description": "Client side common code for coko apps",
  "keywords": [
    "coko",
    "pubsweet"
  ],
  "repository": {
    "type": "git",
    "url": "git@gitlab.coko.foundation:cokoapps/client.git"
  },
  "license": "MIT",
  "author": "Yannis Barlas",
  "main": "dist",
  "bin": {
    "coko-client-build": "./scripts/coko-client-build.sh",
    "coko-client-dev": "./scripts/coko-client-dev.sh"
  },
  "files": [
    "dist",
    "scripts",
    "webpack"
  ],
  "scripts": {
    "build": "npx babel --delete-dir-on-start src --out-dir dist",
    "clean": "rm -rf node_modules && rm -rf dist",
    "cz": "git-cz",
    "devserver": "nodemon -w ./dev/app/theme.js --exec \"sh ./dev/devserver.sh\"",
    "prepublishOnly": "yarn run build",
    "prodserver": "sh ./dev/prodserver.sh",
    "release": "standard-version",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS && lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "dependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.0-beta.6",
    "@pubsweet/client": "^11.0.10",
    "@pubsweet/ui-toolkit": "^2.3.16",
    "app-root-path": "^3.0.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-preset-minify": "^0.5.1",
    "compression-webpack-plugin": "^7.1.2",
    "copy-webpack-plugin": "^8.1.0",
    "css-loader": "^5.2.0",
    "history": "^5.0.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "less": "^4.1.1",
    "less-loader": "^8.1.1",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^1.4.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-refresh": "^0.10.0",
    "react-router-dom": "^5.2.0",
    "style-loader": "^2.0.0",
    "styled-components": "^5.2.3",
    "subscriptions-transport-ws": "^0.9.18",
    "ts-loader": "^8.1.0",
    "url-loader": "^4.1.1",
    "uuid": "^8.3.2",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^4.0.0-beta.2"
  },
  "devDependencies": {
    "@coko/lint": "^1.4.0",
    "antd": "^4.15.4",
    "fontsource-advent-pro": "^4.0.0",
    "http-server": "^0.12.3",
    "nodemon": "^2.0.7",
    "standard-version": "^9.2.0"
  },
  "peerDependencies": {
    "@apollo/client": "^3.3.13",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
