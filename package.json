{
  "name": "@coko/client",
  "version": "0.0.42",
  "description": "Client side common code for coko apps",
  "keywords": [
    "coko",
    "pubsweet"
  ],
  "repository": {
    "type": "git",
    "url": "git@gitlab.coko.foundation:cokoapps/client.git"
  },
  "license": "MIT",
  "author": "Yannis Barlas",
  "main": "dist",
  "bin": {
    "coko-client-build": "./scripts/coko-client-build.sh",
    "coko-client-dev": "./scripts/coko-client-dev.sh",
    "coko-client-dev-js": "./scripts/coko-client-dev.js"
  },
  "files": [
    "dist",
    "scripts",
    "webpack"
  ],
  "scripts": {
    "build": "npx babel --delete-dir-on-start src --out-dir dist",
    "clean": "rm -rf node_modules && rm -rf dist && rm -rf dev/server/node_modules",
    "cz": "git-cz",
    "prepare": "husky install",
    "prepublishOnly": "yarn run build",
    "release": "standard-version",
    "storybook": "STORYBOOK_THEME_PATH='src/theme/index.js' coko-storybook"
  },
  "config": {
    "commitizen": {
      "path": "cz-customizable"
    }
  },
  "resolutions": {
    "postcss": "8",
    "prettier": "2.8.8"
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.0-beta.6",
    "@pubsweet/client": "^11.0.13",
    "@pubsweet/ui-toolkit": "^2.3.17",
    "antd": "^5.6.1",
    "apollo-upload-client": "^11.0.0",
    "app-root-path": "^3.0.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-preset-minify": "^0.5.1",
    "colors": "^1.4.0",
    "compression-webpack-plugin": "^7.1.2",
    "copy-webpack-plugin": "^8.1.0",
    "css-loader": "^5.2.0",
    "history": "^5.0.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^1.4.0",
    "moment": "^2.29.4",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^17.0.2",
    "react-refresh": "^0.10.0",
    "react-router-dom": "^5.2.0",
    "style-loader": "^2.0.0",
    "styled-components": "^5.2.3",
    "styled-normalize": "^8.0.7",
    "subscriptions-transport-ws": "^0.9.18",
    "ts-loader": "^8.1.0",
    "url-loader": "^4.1.1",
    "uuid": "^8.3.2",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^4.0.0-beta.2"
  },
  "devDependencies": {
    "@coko/lint": "^2.0.1",
    "@coko/storybook": "^2.0.0",
    "@faker-js/faker": "^8.0.2",
    "fontsource-advent-pro": "^4.0.0",
    "nodemon": "^2.0.7",
    "standard-version": "^9.2.0",
    "webpack-bundle-analyzer": "^4.9.0"
  },
  "peerDependencies": {
    "@apollo/client": "^3.3.13",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
